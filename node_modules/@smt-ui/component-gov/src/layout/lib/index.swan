<view
    class="
        layout-container gov-layout-container
        {{(isFullScreen && secureBottom) ? 'iphone-x-compitable' : ''}}"
>
    <view class="content"><slot name="container"></slot></view>
    <view class="layout-text gov-layout-text">
        <text>{{text}}</text>
        <view>
            <slot name="text"></slot>
        </view>
    </view>
</view>