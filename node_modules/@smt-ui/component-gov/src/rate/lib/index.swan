<view class="rate gov-rate">
    <view class="rate-container">
        <view
            s-for="item, index in countNum"
            class="rate-icon"
            data-index="{{ index }}"
            bindtap="onChangeSelf"
        >
            <gov-icon
                name="{{ item.icon }}"
                size="{{ icon.size }}"
                color="{{ item.color }}"
            ></gov-icon>
        </view>
    </view>
    <text s-if="showText" class="rate-text gov-rate-text">{{ oText }}</text>
</view>