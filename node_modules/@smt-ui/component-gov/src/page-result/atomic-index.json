{
    "name": "gov-page-result",
    "label": "完成组件",
    "description": "完成页面组件，主要用于表单分步填写完成提交的页面",
    "codeSnap": "<gov-page-result gov-main-btn=\"gov-main-btn\" gov-ext-info-text=\"gov-ext-info-text\" desc=\"这里是一些描述内容\" title=\"预约成功\" main-btn-text=\"返回首页\" sub-btn-text=\"查看详情\" tips=\"{{tips}}\" bind:goback=\"goBack\" bind:viewdetails=\"viewDetails\"></gov-page-result>",
    "groups": "layout",
    "demoCode": {
        "s": "<view>\n    <gov-page-result\n        gov-main-btn=\"gov-main-btn\"\n        gov-ext-info-text=\"gov-ext-info-text\"\n        desc=\"这里是一些描述内容\"\n        title=\"预约成功\"\n        main-btn-text=\"返回首页\"\n        sub-btn-text=\"查看详情\"\n        tips=\"{{tips}}\"\n        bind:goback=\"goBack\"\n        bind:viewdetails=\"viewDetails\"\n    >\n        <view slot=\"suppl-cont\">这是补充说明的内容</view>\n        <view slot=\"bottom-btn\" class=\"other-btn\">\n            <gov-button\n                button-size=\"large\"\n                button-color=\"thirdly\"\n                button-text=\"其它按钮\"\n            ></gov-button>\n        </view>\n    </gov-page-result>\n</view>",
        "js": "Page({\n    data: {\n        tips: '这里是一些提示文案 \n 这里是一些提示文案'\n    },\n    goBack() {\n        swan.showToast({\n            title: '返回首页',\n            icon: 'none'\n        });\n    },\n    viewDetails() {\n        swan.showToast({\n            title: '查看详情',\n            icon: 'none'\n        });\n    }\n});",
        "css": ".gov-main-btn {\n    background-color: green !important;\n}\n\n.gov-ext-info-text {\n    white-space: pre-line;\n}\n\n.other-btn {\n    margin-top: 8px;\n}"
    },
    "fragement": "",
    "properties": {
        "attributes": [
            {
                "name": "imageUrl",
                "type": "String",
                "description": "内容区域的icon图片",
                "required": "否",
                "default": ""
            },
            {
                "name": "showIcon",
                "type": "Boolean",
                "description": "是否显示icon",
                "required": "否",
                "default": "true"
            },
            {
                "name": "iconName",
                "type": "String",
                "description": "icon的名字",
                "required": "否",
                "default": "pass-tn"
            },
            {
                "name": "iconSize",
                "type": "String",
                "description": "icon的大小，默认192px即15.458vw",
                "required": "否",
                "default": "15.458vw"
            },
            {
                "name": "iconColor",
                "type": "Array, String",
                "description": "icon的颜色",
                "required": "否",
                "default": "#2772fb"
            },
            {
                "name": "title",
                "type": "String",
                "description": "内容区域的标题",
                "required": "否",
                "default": ""
            },
            {
                "name": "desc",
                "type": "String",
                "description": "内容区域的描述",
                "required": "否",
                "default": ""
            },
            {
                "name": "contentStyle",
                "type": "Object",
                "description": "内容区域的style样式",
                "required": "否",
                "default": "{}"
            },
            {
                "name": "mainBtnText",
                "type": "String",
                "description": "主按钮文本",
                "required": "否",
                "default": ""
            },
            {
                "name": "subBtnText",
                "type": "String",
                "description": "副按钮文本",
                "required": "否",
                "default": ""
            },
            {
                "name": "tips",
                "type": "String",
                "description": "提示内容",
                "required": "否",
                "default": ""
            },
            {
                "name": "customConWidth",
                "type": "String",
                "description": "自定义内容区宽度",
                "required": "否",
                "default": "460.145rpx"
            }
        ],
        "events": [
            {
                "name": "goback",
                "type": "",
                "description": "点击主按钮时触发",
                "required": "否",
                "default": "-"
            },
            {
                "name": "viewdetails",
                "type": "",
                "description": "点击副按钮时触发",
                "required": "否",
                "default": "-"
            }
        ],
        "slot": [
            {
                "name": "suppl-cont",
                "type": "",
                "description": "补充说明的内容",
                "required": "否",
                "default": "-"
            },
            {
                "name": "bottom-btn",
                "type": "",
                "description": "底部按钮部分的slot",
                "required": "否",
                "default": "-"
            }
        ],
        "externalClasses": [
            {
                "name": "gov-main-btn",
                "type": "",
                "description": "主按钮的扩展样式类，可通过此class改变主按钮的样式",
                "required": "否",
                "default": "-"
            },
            {
                "name": "gov-sub-btn",
                "type": "",
                "description": "副按钮的扩展样式类，可通过此class改变副按钮的样式",
                "required": "否",
                "default": "-"
            },
            {
                "name": "gov-bottom",
                "type": "",
                "description": "底部按钮和提示模块的样式类，可以通过此class改变底部按钮和提示模块外层的样式，如上边距",
                "required": "否",
                "default": "-"
            },
            {
                "name": "gov-ext-info-text",
                "type": "",
                "description": "提示信息的扩展样式，可通过此class改变提示信息的样式，如设置提示内容换行：white-space: pre-line",
                "required": "否",
                "default": "-"
            }
        ]
    }
}