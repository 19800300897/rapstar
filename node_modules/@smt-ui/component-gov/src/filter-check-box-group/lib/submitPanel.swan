<view class="submit-panel gov-submit-panel">
    <gov-button
        class="btn reset"
        button-size="middle"
        button-color="plain"
        button-text="重置"
        gov-button="custom-btn"
        bind:click="reset"
        button-disabled="{{resetDisabled}}"
    />
    <gov-button
        class="btn"
        button-size="middle"
        button-color="primary"
        button-text="确定"
        gov-button="custom-btn"
        bind:click="ensure"
    />
</view>
