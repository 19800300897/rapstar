<view class="serv-entry">
    <view
        class="serv-entry-img gov-serv-entry-img"
        style="background-image:url({{image}})"
    >
        <view class="title gov-title">{{title}}</view>
        <view class="desc gov-desc">{{desc}}</view>
    </view>
    <view
        class="list gov-list"
        s-for="item, index in list"
    >
        <view
            class="list-item gov-list-item"
            data-index="{{index}}"
            bind:tap="clickList"
        >
            <view class="text gov-text">{{item}}</view>
            <gov-icon
                class="arrow-icon"
                name="arrow-right-o"
                color="#ccc"
                size="32.609rpx"
            ></gov-icon>
        </view>
    </view>
    <view class="tel gov-tel">
        {{phonecontent}}ï¼š
        <view
            class="tel-number gov-tel-number"
            bind:tap="clickHotline"
        >{{hotline}}</view>
    </view>
    <view
        class="{{isFullScreen ? 'yes-iphonex' : 'no-iphonex'}}"
        bind:tap="clickBtn"
    >
        <gov-button
            button-size="large"
            button-color="{{bottoncolor}}"
            button-text="{{botton}}"
            button-disabled="{{false}}">
        </gov-button>
    </view>
</view>
