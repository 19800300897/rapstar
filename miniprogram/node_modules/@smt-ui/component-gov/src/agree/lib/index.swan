<view class="container gov-container">
    <view class="agree" bindtap="checkdChange">
        <view class="agree-icon">
            <gov-icon s-if="{{checked}}" name="complete" size="36.23rpx" color="{{iconColor}}"></gov-icon>
            <gov-icon s-if="{{!checked}}" name="radio-uncheck" size="36.23rpx" color="#ccc"></gov-icon>
        </view>
        <view class="agree-desc gov-agree-desc">
            <slot name="text">{{value}}</slot>
        </view>
    </view>
</view>
