.container 
    background var(--color-gray-7, #f5f5f5)
    border-radius var(--top-border-height, 30px)
    opacity .8

    .prompt 
        padding var(--h-spacing-md, 51px)  var(--v-spacing-sm, 42px) 49px

        .prompt-title,.prompt-content 
            font-size var(--font-size-xs, 42px)
            color var(--color-gray-3, #999)

        .prompt-content 
            line-height 67px

        .prompt-title 
            margin-bottom 9px


