<view class="table-content gov-table-content">
    <view class="table-title gov-table-title">
        <view class="table-title-item gov-table-title-item" s-for="item,index in main.title">{{item}}</view>
    </view>
    <view class="content-item-line"></view>
    <block s-for="item,index in main.list">
        <table-item
            main="{{item}}"
            bottom-border="{{index === list.length - 1 ? false : true}}"
        >
        </table-item>
    </block>
</view>