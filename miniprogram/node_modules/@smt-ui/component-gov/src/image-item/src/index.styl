.image-wrap 
    overflow hidden
    position relative

    .opacity-border 
        border 1px solid rgba(0, 0, 0, .1)
        box-sizing border-box
        position absolute
        top 0
        right 0
        width 100%
        height 100%
        vertical-align top
        
    .plate-img 
        width 100%
        height 100%
        vertical-align top

