<div class="filter-page">
    <gov-filter-header selections="{{selections}}" border bind:expand="expandHdl" bind:select="selHdl"/>
    <div scroll-y class="result">
        <gov-filter-body class="filter-body" selections="{{selections}}"/>
        <gov-list-item
            label-width="6em"
            label="选中的值:"
            content="{{selText}}"
        ></gov-list-item>
        <slot></slot>
    </div>
</div>
