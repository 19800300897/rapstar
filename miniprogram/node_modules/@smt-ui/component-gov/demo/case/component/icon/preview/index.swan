<view>
    <view class="tips">温馨提示：点击icon，复制icon的名称</view>
    <view class="gov-icon-page">
        <view class="item" s-for="name in iconName">
            <gov-icon name="{{name}}" size="25vw" bind:tap="tapHdl" data-name="{{name}}" color="#338bff"></gov-icon>
            <text class="caption">{{name}}</text>
        </view>
    </view>
    <view class="gov-icon-page">
        <view class="item" s-for="name in iconNameM">
            <gov-icon name="{{name}}" size="25vw" bind:tap="tapHdl" data-name="{{name}}"></gov-icon>
            <text class="caption">{{name}}</text>
        </view>
    </view>
</view>
